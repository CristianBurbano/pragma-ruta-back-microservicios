{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/pragma-back-microservicios/src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
    ],
    "webpack": true,
    "tsConfigPath": "apps/pragma-back-microservicios/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/pragma-back-microservicios",
  "projects": {
    "pragma-back-microservicios": {
      "type": "application",
      "root": "apps/pragma-back-microservicios",
      "entryFile": "main",
      "sourceRoot": "apps/pragma-back-microservicios/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pragma-back-microservicios/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "images": {
      "type": "application",
      "root": "apps/images",
      "entryFile": "main",
      "sourceRoot": "apps/images/src",
      "compilerOptions": {
        "tsConfigPath": "apps/images/tsconfig.app.json"
      }
    }
  }
}